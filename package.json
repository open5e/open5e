{
  "name": "nuxt-app",
  "private": true,
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prepare": "husky install",
    "lint": "npm run lint:js && npm run lint:prettier",
    "lint:ci": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:js": "eslint --ext '.js,.vue' --ignore-path .gitignore .",
    "lint:prettier": "prettier --check --ignore-path .gitignore '**/*.{js,vue}'",
    "lintfix": "npm run lint:js -- --fix && npm run lint:prettier -- --write",
    "pre-commit": "npx pretty-quick --staged && npm run lintfix",
    "test": "echo 'Coming soon, hopefully!'"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxtjs/tailwindcss": "^6.7.0",
    "@types/node": "^18",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.13.0",
    "husky": "^8.0.3",
    "nuxt": "^3.5.0",
    "nuxt-icon": "^0.4.1",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "pretty-quick": "^3.1.3",
    "sass": "^1.62.1"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.14",
    "@heroicons/vue": "^2.0.18",
    "@pinia/nuxt": "^0.4.11",
    "axios": "^1.6.0",
    "pinia": "^2.1.3",
    "tailwindcss": "^3.3.2",
    "underscore": "^1.13.6",
    "vue": "^3.3.4",
    "vue-gtag-next": "^1.14.0",
    "vue-showdown": "^4.1.0"
  }
}
