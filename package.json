{
  "name": "nuxt-app",
  "private": true,
  "engines": {
    "node": "20.x"
  },
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prepare": "husky install",
    "format": "npx prettier --write .",
    "lint": "npm run lint:js && npm run lint:prettier",
    "lint:ci": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:js": "eslint --ext '.js,.vue' --ignore-path .gitignore .",
    "lint:prettier": "prettier --check --ignore-path .gitignore '**/*.{js,vue}'",
    "lintfix": "npm run lint:js -- --fix && npm run lint:prettier -- --write",
    "pre-commit": "npx pretty-quick --staged && npm run lintfix",
    "test": "npx vitest run --silent",
    "start": "node .output/server/index.mjs"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxt/test-utils": "^3.13.1",
    "@nuxtjs/tailwindcss": "^6.7.0",
    "@types/node": "^18",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.13.0",
    "happy-dom": "^14.12.3",
    "husky": "^8.0.3",
    "nuxt": "^3.12.4",
    "nuxt-icon": "^0.4.1",
    "playwright-core": "^1.45.1",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "pretty-quick": "^3.1.3",
    "sass": "^1.62.1",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.14",
    "@hebilicious/vue-query-nuxt": "^0.3.0",
    "@heroicons/vue": "^2.0.18",
    "@tanstack/vue-query": "^5.35.1",
    "@vueuse/core": "^10.11.0",
    "axios": "^1.7.4",
    "tailwindcss": "^3.3.2",
    "vue": "^3.3.4",
    "vue-gtag-next": "^1.14.0",
    "vue-showdown": "^4.1.0"
  }
}
